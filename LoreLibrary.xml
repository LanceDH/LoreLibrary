	<Frame name="LOLIB_MapPinTemplate" virtual="true">
		<Size x="16" y="16"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="glow" file="Interface\MINIMAP\Ping\ping5" hidden="true" alphaMode="ADD">
					<Size x="24" y="24"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<Texture parentKey="tex" file="Interface\AddOns\LoreLibrary\Images\icon_Object" />
			</Layer>
		</Layers>
		
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
				if (self.lore) then
					GameTooltip:SetText((self.lore.translation or self.lore.title));
					if (self.lore.unlocked) then
						GameTooltip:AddLine("Unlocked" ,1 ,1 ,1 ,true);
						GameTooltip:Show();
					end
				end
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Frame>
	
